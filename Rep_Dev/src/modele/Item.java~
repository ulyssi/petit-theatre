
public class Item{
        public Spectacle spectacle;
    public Representation reprensentation;
    public ArrayList<Places> lesPlaces;
    public Item(Spectacle s , Reprensentation r ){
	spectacle=s; 
	reprensentation=r;
	lesPlaces= new<lesPlaces> ArrayList();
    }
    public String toString(){
	res=""+spectacle.getNom()+" "+ spectacle.getNum()+"\n";
	return res;
	// prixres+=reprensentation.
	
	
    }
    
}